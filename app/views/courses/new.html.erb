<h1>New Course</h1>

<%= form_for :course, url:courses_path do |f| %>
  <p>
    <%= f.label :course %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :teacher %><br>
    <%= f.text_area :teacher %>
  </p>
  <p>
    <%= f.label :time %><br>
    <%= f.text_field :time %>
  </p>

  <%=f.submit %>
<% end %>
<%= link_to 'Back', courses_path %>